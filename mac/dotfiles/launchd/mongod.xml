<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>local.mongod</string>
    <key>ProgramArguments</key>
    <array>
      <string>mongod</string>
      <string>--config=/Users/amy/.config/mongodb/mongod.conf</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <true/>
    </dict>
    <!-- debugging -->
    <key>StandardErrorPath</key>
    <string>/tmp/mongod-service.log.err</string>
    <key>StandardOutPath</key>
    <string>/tmp/mongod-service.log.out</string>
  </dict>
</plist>
